# LowKey™ - Primal Genesis Engine™ Edition
# Environment Configuration Example
# Copy this file to .env.local and update with your values

# =============================================================================
# PRIMAL GENESIS ENGINE CONFIGURATION
# =============================================================================

# Authority Level Configuration
PRIMAL_GENESIS_AUTHORITY=primary
QUANTUM_PROCESSING_ENABLED=true
DIVINE_FLOW_ENABLED=true

# Quantum Processing Settings
QUANTUM_ENTANGLEMENT_ENABLED=true
QUANTUM_SUPERPOSITION_MODE=active
QUANTUM_TUNNELING_THRESHOLD=0.85

# Divine Flow Configuration
DIVINE_FLOW_OPTIMIZATION=true
COSMIC_PRECISION_MODE=enabled
AUTHORITY_HARMONIZATION=true

# =============================================================================
# USER OVERRIDE CONFIGURATION
# =============================================================================

# Override Authority Settings
USER_OVERRIDE_AUTHORITY=ultimate
INSTANT_OVERRIDE_ENABLED=true
EMERGENCY_CONTROL_ENABLED=true

# Override Response Times (in milliseconds)
OVERRIDE_RESPONSE_TIME=10
EMERGENCY_RESPONSE_TIME=5
AUTHORITY_HANDOFF_TIME=3

# Override Security
OVERRIDE_AUTHENTICATION_REQUIRED=true
OVERRIDE_AUDIT_LOGGING=true
OVERRIDE_ENCRYPTION_LEVEL=quantum

# =============================================================================
# VEHICULAR AUTHORITY CONFIGURATION
# =============================================================================

# Vehicle Integration Settings
VEHICULAR_AUTHORITY_ENABLED=true
CAN_BUS_INTEGRATION=true
AUTONOMOUS_CONTROL_ENABLED=true

# Supported Vehicle Configurations
MERCEDES_S_CLASS_ENABLED=true
CADILLAC_ESCALADE_ENABLED=true
DODGE_DEMON_ENABLED=true
BMW_M3_ENABLED=true
TESLA_MODEL_Y_ENABLED=true

# CAN Bus Communication
CAN_BUS_TIMEOUT=1000
CAN_BUS_RETRY_ATTEMPTS=3
CAN_BUS_ENCRYPTION=true

# Vehicle Authority Granularity
ENGINE_CONTROL_AUTHORITY=true
TRANSMISSION_AUTHORITY=true
SECURITY_AUTHORITY=true
ENTERTAINMENT_AUTHORITY=true
CLIMATE_AUTHORITY=true
LIGHT_AUTHORITY=true

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Quantum Security Settings
QUANTUM_ENCRYPTION_ENABLED=true
QUANTUM_KEY_DISTRIBUTION=true
QUANTUM_RESISTANT_ALGORITHMS=true

# Authority Security
AUTHORITY_AUDIT_ENABLED=true
AUTHORITY_ISOLATION=true
AUTHORITY_BACKUP_ENABLED=true
AUTHORITY_RECOVERY_ENABLED=true

# Security Logging
SECURITY_LOGGING_ENABLED=true
SECURITY_LOG_LEVEL=info
SECURITY_LOG_RETENTION_DAYS=90

# Authentication Settings
AUTHENTICATION_METHOD=multi_factor
SESSION_TIMEOUT_MINUTES=30
MAX_LOGIN_ATTEMPTS=5

# =============================================================================
# PROXIMITY SUITE CONFIGURATION
# =============================================================================

# BLE Scanning Settings
BLE_SCANNING_ENABLED=true
BLE_SCAN_INTERVAL=1000
BLE_SCAN_WINDOW=500
BLE_FILTER_ENABLED=true

# NFC Configuration
NFC_SCANNING_ENABLED=true
NFC_READ_TIMEOUT=5000
NFC_WRITE_TIMEOUT=10000

# Proximity Correlation
PROXIMITY_CORRELATION_ENABLED=true
PROXIMITY_THRESHOLD_METERS=5.0
PROXIMITY_UPDATE_INTERVAL=1000

# =============================================================================
# PHANTOM TOUCH CONFIGURATION
# =============================================================================

# RFID Processing
RFID_CAPTURE_ENABLED=true
RFID_EMULATION_ENABLED=true
RFID_SIGNAL_PROCESSING=true

# Signal Processing
SIGNAL_FILTERING_ENABLED=true
SIGNAL_AMPLIFICATION=true
SIGNAL_NOISE_REDUCTION=true

# Protocol Detection
PROTOCOL_DETECTION_ENABLED=true
PROTOCOL_ANALYSIS_DEPTH=deep
PROTOCOL_LOGGING=true

# =============================================================================
# GHOSTSKIN CONFIGURATION
# =============================================================================

# Signal Scrambling
SIGNAL_SCRAMBLING_ENABLED=true
SCRAMBLING_ALGORITHM=quantum_chaos
SCRAMBLING_INTENSITY=maximum

# Stealth Mode
STEALTH_MODE_ENABLED=true
STEALTH_DETECTION_EVASION=true
STEALTH_SIGNATURE_MASKING=true

# =============================================================================
# WHISPERGATE CONFIGURATION
# =============================================================================

# AI Authentication
AI_AUTHENTICATION_ENABLED=true
VOICE_RECOGNITION_ENABLED=true
FACIAL_RECOGNITION_ENABLED=true

# AthenaMist Integration
ATHENAMIST_ENABLED=true
ATHENAMIST_API_KEY=your_athenamist_api_key_here
ATHENAMIST_ENDPOINT=https://api.athenamist.com/v1

# =============================================================================
# DIVINE REACH CONFIGURATION
# =============================================================================

# Cross-Device Synchronization
DEVICE_SYNC_ENABLED=true
SYNC_INTERVAL_SECONDS=30
SYNC_ENCRYPTION=true

# Device Registry
DEVICE_REGISTRY_ENABLED=true
DEVICE_AUTHENTICATION=true
DEVICE_AUTHORIZATION=true

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Database Connection
DATABASE_URL=postgresql://username:password@localhost:5432/lowkey_db
DATABASE_SSL=true
DATABASE_POOL_SIZE=10

# Database Encryption
DATABASE_ENCRYPTION_ENABLED=true
DATABASE_ENCRYPTION_KEY=your_database_encryption_key_here

# =============================================================================
# API CONFIGURATION
# =============================================================================

# API Settings
API_PORT=3000
API_HOST=localhost
API_CORS_ENABLED=true
API_RATE_LIMITING=true

# API Security
API_AUTHENTICATION_REQUIRED=true
API_JWT_SECRET=your_jwt_secret_here
API_JWT_EXPIRY_HOURS=24

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================

# Logging Settings
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUT=file

# Log File Configuration
LOG_FILE_PATH=./logs/lowkey.log
LOG_FILE_MAX_SIZE=100MB
LOG_FILE_MAX_FILES=10

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================

# Development Mode
NODE_ENV=development
DEBUG_ENABLED=true
HOT_RELOAD_ENABLED=true

# Testing Configuration
TEST_DATABASE_URL=postgresql://test_user:test_password@localhost:5432/lowkey_test
TEST_MODE_ENABLED=false

# =============================================================================
# EXTERNAL INTEGRATIONS
# =============================================================================

# Third-Party Services
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_ENABLED=false
TELEMETRY_ENABLED=false

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password_here

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================

# Performance Settings
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
CACHE_MAX_SIZE=1000

# Memory Management
MEMORY_LIMIT_MB=2048
GARBAGE_COLLECTION_ENABLED=true

# =============================================================================
# MONITORING CONFIGURATION
# =============================================================================

# Health Checks
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL_SECONDS=30

# Metrics Collection
METRICS_ENABLED=true
METRICS_PORT=9090

# Alerting
ALERTING_ENABLED=true
ALERT_EMAIL=alerts@lowkey-project.com

# =============================================================================
# BACKUP CONFIGURATION
# =============================================================================

# Backup Settings
BACKUP_ENABLED=true
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=30

# Backup Storage
BACKUP_STORAGE_TYPE=local
BACKUP_STORAGE_PATH=./backups
BACKUP_ENCRYPTION=true

# =============================================================================
# COMPLIANCE CONFIGURATION
# =============================================================================

# Data Protection
GDPR_COMPLIANCE_ENABLED=true
DATA_RETENTION_DAYS=90
DATA_ANONYMIZATION_ENABLED=true

# Audit Trail
AUDIT_TRAIL_ENABLED=true
AUDIT_LOG_RETENTION_DAYS=365
AUDIT_LOG_ENCRYPTION=true 